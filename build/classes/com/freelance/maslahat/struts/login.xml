<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="Login" namespace="/" extends="default">	
		<action name="login">
			<result>/WEB-INF/pages/login.jsp</result>
		</action>
		<action name="loginAction" class="com.maslahat.action.LoginAction">
			<result name="success">/WEB-INF/pages/dashboard/dashboard.jsp</result>
		</action>
	</package>
	
</struts>
